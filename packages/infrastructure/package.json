{
  "name": "@alg-wiki/infrastructure",
  "version": "0.0.1",
  "description": "alg.wiki backend infrastructure.",
  "private": true,
  "keywords": [],
  "homepage": "https://github.com/alg-wiki/monorepo#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/alg-wiki/monorepo.git",
    "directory": "packages/infrastructure"
  },
  "bugs": {
    "url": "https://github.com/alg-wiki/monorepo/issues"
  },
  "author": "jakzo",
  "license": "MIT",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=14"
  },
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "scripts": {
    "init-pulumi-local": "ts-node --project ../../tsconfig.json --transpile-only --compiler-options '{ \"module\": \"CommonJS\" }' ./scripts/init-pulumi-local.ts",
    "pulumi": "PULUMI_CONFIG_PASSPHRASE=$(cat .pulumi-passphrase) pulumi",
    "pulumi-up": "tsc --build && PULUMI_CONFIG_PASSPHRASE=$(cat .pulumi-passphrase) pulumi up",
    "localstack": "yarn init-pulumi-local && docker-compose up"
  },
  "dependencies": {
    "@alg-wiki/db": "workspace:packages/db",
    "@pulumi/aws": "^4.0.0",
    "@pulumi/awsx": "^0.30.0",
    "@pulumi/pulumi": "^3.0.0",
    "aws-lambda": "^1.0.6",
    "fast-glob": "^3.2.7",
    "runtypes": "^6.4.1",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.56.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/node": "14.14.37",
    "fs-extra": "^10.0.0",
    "ts-node": "^10.2.1"
  }
}
