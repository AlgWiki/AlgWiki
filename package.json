{
  "name": "alg-wiki",
  "version": "0.0.2",
  "description": "Computer science algorithm database",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently --kill-others -n c,s -c blue.dim,magenta.dim \"yarn dev-client\" \"yarn dev-server\"",
    "dev-client": "yarn clean && parcel watch ./src/client/index.html -d ./public",
    "dev-server": "ts-node-dev --inspect=11039 --project ./tsconfig.server.json ./src/index.ts",
    "build": "yarn build-client",
    "build-client": "yarn clean && parcel build ./src/client/index.html -d ./public",
    "start": "ts-node --project ./tsconfig.server.json ./src/index.ts",
    "test": "jest",
    "clean": "rimraf ./public ./.cache",
    "prepublish": "yarn build"
  },
  "author": "jakzo",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "concurrently": "^3.5.1",
    "jest": "^23.1.0",
    "parcel-bundler": "^1.9.0",
    "postcss-modules": "^1.1.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.10.0"
  },
  "dependencies": {
    "@atlaskit/button": "^8.2.0",
    "@atlaskit/css-reset": "^2.0.6",
    "@atlaskit/editor-common": "^11.2.1",
    "@atlaskit/editor-core": "^73.7.5",
    "@atlaskit/media-core": "^19.1.3",
    "@atlaskit/size-detector": "^4.1.2",
    "@atlaskit/spinner": "^7.1.0",
    "@atlaskit/tabs": "^7.1.2",
    "@atlaskit/theme": "^4.0.4",
    "@types/codemirror": "^0.0.57",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.28",
    "@types/koa-static": "^4.0.0",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "@types/react-loadable": "^5.4.0",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/uuid": "^3.4.3",
    "codemirror": "^5.39.0",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "koa-sendfile": "^2.0.0",
    "koa-static": "^4.0.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-monaco-editor": "^0.17.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.2",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.2",
    "uuid": "^3.2.1"
  }
}
