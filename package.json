{
  "name": "alg-wiki",
  "version": "0.0.2",
  "description": "Computer science algorithm database",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently --kill-others -n d,c,s -c red.dim,blue.dim,magenta.dim \"yarn db\" \"yarn watch-client\" \"yarn watch-server\"",
    "dev-debug": "concurrently --kill-others -n d,c,s -c red.dim,blue.dim,magenta.dim \"yarn db\" \"yarn watch-client\" \"yarn debug-server\"",
    "db": "ts-node --project ./tsconfig.server.json ./scripts/db.ts 11038 ./db-data",
    "build": "yarn clean && TS_NODE_PROJECT=\"./tsconfig.server.json\" webpack",
    "watch-client": "yarn build -w",
    "watch-server": "ts-node-dev --project ./tsconfig.server.json ./src/index.ts",
    "debug-server": "ts-node-dev --inspect=11039 --inspect-brk --project ./tsconfig.server.json ./src/index.ts",
    "start": "ts-node --project ./tsconfig.server.json ./src/index.ts",
    "test": "jest",
    "clean": "rimraf ./public"
  },
  "author": "jakzo",
  "license": "MIT",
